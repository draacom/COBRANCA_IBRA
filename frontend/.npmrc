# Configurações NPM para resolver problemas de compatibilidade
legacy-peer-deps=true
fund=false
audit=false
progress=false
loglevel=error

# Configurações para ambientes de produção
production=false
optional=true
dev=true

# Resolver problemas de permissão
unsafe-perm=true

# Cache e registry
cache-min=86400
registry=https://registry.npmjs.org/